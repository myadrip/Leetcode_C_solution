//https://leetcode.com/problems/unique-paths/



int uniquePaths(int m, int n)
{
    int** buf = (int**)malloc(sizeof(int*)*m);
    for(int i = 0; i < m; i++)
    {
        buf[i] = (int*)malloc(sizeof(int)*n);
        memset(buf[i], 0x00, sizeof(int)*n);
    }
    
    for(int i = 0; i < m; i++)
    {
        for(int j = 0; j < n; j++)
        {
            if(i==0 && j==0)
            {
                buf[i][j] = 1;
            }
            else if(i==0)
            {
                buf[i][j] = buf[i][j-1];
            }
            else if(j==0)
            {
                buf[i][j] = buf[i-1][j];
            }
            else
            {
                buf[i][j] = buf[i][j-1] + buf[i-1][j];
            }
        }
    }
    int rv = buf[m-1][n-1];
    for(int i = 0; i < m; i++)
        free(buf[i]);
    free(buf);
    
    return rv;
    

}
