//https://leetcode.com/problems/counting-bits/

int* countBits(int num, int* returnSize)
{
    *returnSize  =num+1;
    int* rv = (int*)calloc((*returnSize), sizeof(int));
    
    int n = 0;
    for(int i = 0; i <=num; i++)
    {
        n = i;
        while(n>0)
        {
            if(n&1)
                rv[i]++;
            n=n>>1;
        }
    }
    
    return rv;
}
