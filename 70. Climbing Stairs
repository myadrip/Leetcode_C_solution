//https://leetcode.com/problems/climbing-stairs/



int Fib(int n, int* buf)
{
    if(buf[n] != -1)
        return buf[n];
    else
    {
        buf[n] = Fib(n-1, buf)+Fib(n-2, buf);
        return buf[n];
    }
}

int climbStairs(int n)
{
    int* buf = (int*)malloc(sizeof(int)*(n+1));
    for(int i = 0; i < (n+1); i++)
    {
        if(i == 0 || i == 1)
            buf[i] = 1;
        else
            buf[i] = -1;
    }
        
    
    int rv = Fib(n, buf);    
    
    free(buf);    
    return rv;
}
