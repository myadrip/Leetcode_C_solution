//https://leetcode.com/problems/climbing-stairs/



int climbStairs(int n)
{
    int* dp = (int*)malloc(sizeof(int)*(n+1));
    
    for(int i = 0; i < n+1; i++)
    {
        if(i<2)
            dp[i] = 1;
        else
        {
            dp[i] = dp[i-1] + dp[i-2];
        }
        
    }
    
    int ret = dp[n];
    free(dp);
    
    return ret;

}


